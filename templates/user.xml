<templateSet group="user">
  <template name="it" value="it('should $SHOULD$', function($PARAMS$){&#10;  $END$&#10;});" description="creates an it test" toReformat="true" toShortenFQNames="true">
    <variable name="SHOULD" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="PARAMS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="hbslog" value="{{log &quot;**********************************&quot;}}&#10;{{log &quot;$FIELDS$: &quot;}}{{log $FIELDS$}}$END$" description="Inserts a labelled handlebars {{log}} statement" toReformat="true" toShortenFQNames="true">
    <variable name="FIELDS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
    </context>
  </template>
  <template name="log" value="console.log('$VALUE$: ', $VALUE$);$END$" description="Inserts a console.log statement" toReformat="false" toShortenFQNames="true">
    <variable name="VALUE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="dir" value="console.dir('$VALUE$: ', $VALUE$);$END$" description="Inserts a console.dir statement" toReformat="false" toShortenFQNames="true">
    <variable name="VALUE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="hbslg" value="{{log &quot;**********************************&quot;}}&#10;{{log &quot;$FIELDS$: &quot;}}{{log $FIELDS$}}$END$" description="Inserts a labelled handlebars {{log}} statement" toReformat="true" toShortenFQNames="true">
    <variable name="FIELDS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
    </context>
  </template>
  <template name="lg" value="console.log('$VALUE$: ', $VALUE$);$END$" description="Inserts a console.log statement" toReformat="false" toShortenFQNames="true">
    <variable name="VALUE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>